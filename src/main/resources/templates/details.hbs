{{#partial "content"}}
<h5>Team Name:</h5>
<p>{{foundEntry.name}}</p>
<h5>Description:</h5>
<p>{{foundEntry.description}}</p>
<h5>Member:</h5>
<p>{{foundEntry.member}}</p>


{{#if published }}
<span class="glyphicon glyphicon-star" aria-hidden="true"></span>  Published
{{else}}
<span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> Draft
{{/if}}

<a href="/team/{{foundEntry.memberId}}/edit">Edit this post/add more members</a><br>
<!--<a href="//{{foundEntry.addressId}}/delete">DELETE this post (no undo)</a>-->
</p>
{{/partial}}

{{> layout.hbs}}